    "@types/node": "^14.0.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.3"
  }
}
